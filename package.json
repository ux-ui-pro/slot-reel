    "eslint-plugin-import": "2.31.0",
    "globals": "16.0.0",
    "prettier": "3.5.2",
    "rimraf": "6.0.1",
    "typescript": "5.8.2",
    "vite": "6.2.0",
    "vite-plugin-dts": "4.5.1"
